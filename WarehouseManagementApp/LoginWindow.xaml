<Window x:Class="WarehouseManagementApp.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="300" Width="400">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Email:" Margin="0,0,0,5"/>
            <TextBox x:Name="EmailTextBox" Width="200" Margin="0,0,0,10"/>
            <TextBlock Text="Password:" Margin="0,0,0,5"/>
            <PasswordBox x:Name="PasswordBox" Width="200" Margin="0,0,0,10"/>
            <Button Content="Login" Width="100" Click="LoginButton_Click"/>
        </StackPanel>
    </Grid>
</Window>