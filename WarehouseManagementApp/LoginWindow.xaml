<Window x:Class="WarehouseManagementApp.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="300" Width="400">
    <StackPanel Margin="20">
        <TextBlock Text="Email" Margin="0,0,0,5" />
        <TextBox x:Name="EmailTextBox" />

        <TextBlock Text="Password" Margin="0,20,0,5" />
        <PasswordBox x:Name="PasswordBox" />

        <Button Content="Login" Width="100" Height="30" Margin="0,20,0,0" Click="LoginButton_Click" />
    </StackPanel>
</Window>
